<h1>Admin::Foods#new</h1>
<p>Find me in app/views/admin/foods/new.html.erb</p>

<%= form_with model: @food, url: admin_foods_path do |f| %>
  <div class="mb-3">
    <%= f.label :name %>
    <%= f.text_field :name, class: "form-control" %>
  </div>
  <div class="mb-3">
    <%= f.label :description %>
    <%= f.text_field :description, class: "form-control" %>
  </div>
  <div class="mb-3">
    <%= f.label :price %>
    <%= f.text_field :price, class: "form-control" %>
  </div>

  <%= f.submit "Save", class: "btn btn-primary" %>
<% end %>
